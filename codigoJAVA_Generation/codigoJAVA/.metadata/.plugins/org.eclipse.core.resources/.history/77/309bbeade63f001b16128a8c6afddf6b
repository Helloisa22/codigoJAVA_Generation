package Herança_Encapsulamento;

public class TestePessoa {

	public static void main(String[] args) {
		Pessoa people = new Pessoa("João","Rua são Mariano","40028922",'M', 18);
		
		System.out.printf("Nome %s endereço %s telefone %s do sexo %s com idade %d \n", people.getNome(), people.getEndereco(), people.getTelefone(), people.getSexo(), people.getIdade());
		
		
		Empregado work = new Empregado("João","Rua são Mariano","40028922",'M', 18);
		
		System.out.printf("");
	}

}
