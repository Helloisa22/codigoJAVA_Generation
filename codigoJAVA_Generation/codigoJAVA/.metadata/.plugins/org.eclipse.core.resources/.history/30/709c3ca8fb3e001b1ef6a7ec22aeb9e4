package AtividadeClasseObjeto;

//CLASSE
public class Cliente {
	
	//ATRIBUTOS
	String nome;
	String cpf;
	int tamanho;
	
	//METODOS
	public Cliente(String nome, String cpf, int tamanho) {
		
	}
	
	public Cliente(String nome, String cpf) {
		
		this.nome = nome;
		this.cpf = cpf;
	}
	
	public Cliente(int tamanho) {
		
		this.tamanho = tamanho;
	}
}
